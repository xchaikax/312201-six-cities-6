{
  "name": "six-cities",
  "version": "5.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "start:dev": "nodemon",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts --fix",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "node --loader ts-node/esm",
    "mock:server": "json-server ./mocks/mock-server-data.json --port=3123",
    "mock:generate": "npm run ts ./src/main.cli.ts -- --generate 100 ./mocks/test-data.tsv http://localhost:3123/api",
    "mock:import": "npm run ts ./src/main.cli.ts -- --import ./mocks/test-data.tsv",
    "docker:up": "docker compose --file ./docker-compose.dev.yaml --env-file ./.env --project-name six-cities up -d"
  },
  "dependencies": {
    "@typegoose/typegoose": "11.5.0",
    "chalk": "5.3.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "got": "13.0.0",
    "http-status-codes": "2.3.0",
    "inversify": "6.0.1",
    "jose": "4.15.4",
    "mime-types": "2.1.35",
    "multer": "1.4.5-lts.1",
    "nodemon": "3.0.1",
    "pino": "8.15.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/convict": "6.1.4",
    "@types/convict-format-with-validator": "6.0.3",
    "@types/express": "4.17.18",
    "@types/mime-types": "2.1.2",
    "@types/multer": "1.4.8",
    "@types/node": "18.19.0",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "0.17.3",
    "mongoose": "7.5.3",
    "pino-pretty": "10.2.0",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  }
}
